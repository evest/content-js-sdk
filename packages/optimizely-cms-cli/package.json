{
  "name": "@optimizely/cms-cli",
  "version": "1.0.0",
  "description": "CLI application for integration with Optimizely CMS",
  "repository": {
    "url": "https://github.com/episerver/content-js-sdk.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "test": "vitest --typecheck"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "optimizely",
    "cms",
    "content-management",
    "cli",
    "content-types",
    "headless-cms",
    "code-first"
  ],
  "author": "Optimizely",
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.7.0",
  "type": "module",
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.25.9",
    "@optimizely/cms-sdk": "workspace:*",
    "@inquirer/prompts": "^7.4.0",
    "@oclif/core": "^4.2.10",
    "chalk": "^5.4.1",
    "conf": "^13.1.0",
    "dotenv": "^16.5.0",
    "esbuild": "^0.25.8",
    "glob": "^11.0.1",
    "openapi-fetch": "^0.13.5",
    "ora": "^8.2.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^18",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.2",
    "vitest": "^3.1.1"
  },
  "bin": {
    "optimizely-cms-cli": "./bin/run.js"
  },
  "oclif": {
    "bin": "optimizely-cms-cli",
    "commands": "./dist/commands",
    "dirname": "optimizely-cms-cli",
    "topicSeparator": " ",
    "hooks": {
      "init": "./dist/hooks/init/logo"
    },
    "topics": {
      "config": {
        "description": "Manage content type definitions and sync with CMS"
      },
      "content": {
        "description": "Content type operations"
      },
      "danger": {
        "description": "⚠️  Destructive operations - use with caution"
      }
    }
  }
}
